var contador = numeros.count
var co = 0
var cont = numeros[co]
let var5 : double = co % 5
let var2 : double = co % 2
let dif : bool = var2 != 0
var bingo = "Bingo !!!"
var par = " par"
var impar = “impar”
print(bingo)
print(numeros.count)




for var co = 0; co < numeros.count;  ++co{ 
    if var5 == 0 {
    print("\(cont),\t, (bingo)")
}     else  if var2 == 0 {
print("#\t \(cont) "par")
}     else if dif {
    print("#\t \(cont) "impar")
}     else if co > 29 || co < 41 {
    print ( "#\t \(cont), "Viva Swift!!!")
        
    }
++co
}




